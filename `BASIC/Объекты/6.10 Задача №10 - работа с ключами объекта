
var first = [
    {
        name: `Person`,
        age: 123,
        salary: 1542.33,
        contacts: {
            phone: `112`,
            email: "email@domain.com"
        },
        address: `Moldova`
    }




];
var second = [



    {
        name: `Person`,
        age: 123,
        salary: 1542.33,
        contacts: {
            phone: `112`,
            email: "email@domain.com"
        },
        address: `Moldova`
    }


];

var common = first.filter(x => second.indexOf(x) !== -1)
console.log("The common elements are: " + common);

console.log(common);
